Do !15(Mixes)
    WaitMsec $mixDelay1
    LiqIn #V_MAG_PASS #P_SPEED_M
    WaitMsec $mixDelay2
    LiqOut #V_MAG_PASS #P_SPEED_M
Loop

(10-8-8)[MagDown]
