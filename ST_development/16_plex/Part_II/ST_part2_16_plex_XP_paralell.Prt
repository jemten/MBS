LiqOut #V_MAG_PASS #P_SPEED_LL
WaitMsec $500msec
;SideY% -90 #Y_SPEED_M
;To accommodate for MBS 8000 the Y position was changed to an absolute value in the other direction
AMove Y 17654 W
WaitMsec $mixDelay2
LiqOutAll #P_SPEED_LL
WaitSec $1sec
LiqOut $xpAirVol #P_SPEED_H
Org P

