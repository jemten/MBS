(10-8-3)[GoEthanol]
AddToLogFile ">(10-8-3)"
AddToLogFile "Go ethanol"
MoveTo ETOH $DispIndex
GVal xpTemp1 Let @WellVolume
If $xpTemp1 LT 300
	MoveTo ETOH2 $DispIndex
	GVal xpTemp1 Let @WellVolume
EndIf
If $xpTemp1 GT 1800
	Down% 30 #Z_SPEED_H
ElseIf $xpTemp1 GT 1400
	Down% 50 #Z_SPEED_H
ElseIf $xpTemp1 GT 1000
	Down% 75 #Z_SPEED_H
Else	
	Call LowerTipsMore
EndIf
	
(10-8-4)[AspEthanol]
AddToLogFile ">(10-8-4)"
LiqInWell $xpEthanolVol #P_SPEED_L
WaitSec $1sec
Down% 10 #Z_SPEED_M
SideY% 100 #Y_SPEED_H
;SideY% -100 #Y_SPEED_H
SideY% 0 #Y_SPEED_M
Amove Z 0 W

(10-8-5)[GoWASTE]
AddToLogFile ">(10-8-5)"
If $DispIndex EQ 1
	MoveTo WASTE
Else 
	MoveTo WASTE2
EndIf

(10-8-6)[GoDown%]
