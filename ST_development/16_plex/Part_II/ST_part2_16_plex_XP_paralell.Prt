Gval maxMixVol - 5                     ; A safety margin

; Repeat washing
Do $tipWashRepeats
    Call PreAspiration
    MoveTo OIL_WASH
    ActionZ #Z_SPEED_H
    LiqInWell $tipWashVol #P_SPEED_M
    WaitSec $1sec
    AMove Z 0
    MoveTo WASTE
    Do 5
        LiqIn $maxMixVol #P_SPEED_H
        WaitMsec $mixDelay
        LiqOut $maxMixVol #P_SPEED_H
        WaitMsec $mixDelay
    Loop
    ActionZ #Z_SPEED_M
    LiqOutAll #P_SPEED_L
    LiqOut $preAspirationVol #P_SPEED_H     ; Blow-out
    Org P
    WaitSec $1sec
    SideY% -90 #Y_SPEED_M
    SideY% 0 #Y_SPEED_M
    AMove Z 0
Loop

; Air-dry tips
Gval maxMixVol Let @MaxTipVolume
Gval maxMixVol - 10
StandbyZ #Z_SPEED_H
Do 10
    LiqIn $maxMixVol #P_SPEED_H
    WaitMsec $mixDelay
    LiqOut $maxMixVol #P_SPEED_H
    WaitMsec $mixDelay
Loop
WaitSec $1sec
AMove Z 0
Org P

; Washing complete
Return

(9-3-2)[OilRoutine2]
; String variables that must be set
; Disps oil higher up and against the sidewall 
; $OilPosition
; $DispPosition
; $DispIndex
AddToLogFile ">(9-3-2)"
