; ====================================================== Agencourt_PURIF
; The CA-BEAD_PURIF subroutine has been modified to suite Agencourt RNAClean XP (XP)
; Added a second capture for volumes just above 200 ul. 151005 AJ
; Added a split of bead and sample for even bigger volumes

(10-1-1)[AgencourtPurification]
AddToLogFile ">(10-1-1)"
; modifying the XP protocol to use tipwellvol for liquids and tipvol for air
; 150905 AJ

; String variables must be set before call:
; $cntXPStartingPos = Which plate (row) contains the sample
; $cntXPWorkingPos = Which plate (row) the beads and samples should incubate
; $cntXPElutionPos = Which plate (row) should be eluted in

; $cntXPBeadVol = amount of XP beads
; $cntXPElutionVol = Elution volume
; $cntPostXPTemp = Sets the temperature for next step when the elution starts
; $cntBufferPos = alternative position for the elution buffer 
; $DispIndex = index working position

AddToLogFile "Starting RNAClean purification protocol"
AddToLogFile "Starting position: " $cntXPStartingPos
AddToLogFile "Elution position: " $cntXPElutionPos

; Reset position
GoHome
Call HomeMotors
Org P W
ClearTipVolume
Gval cntXPElutionVol Add 1

; Reserving tips for paralell processing
Gval cntTipID1 Inc
Gval cntTipID2 Inc
ReserveTip Standard $cntTipID1
ReserveTip Standard $cntTipID2
GetReserveTip $cntTipID1
Amove P 0 W
Org P

(10-1-2)[Splitting volumes]
AddToLogFile ">(10-1-2)"
If $DoSplit EQ 1
	Gval SplitBeadsVol Let $cntXPBeadVol
	Gval SplitBeadsVol Div 2
	Gval GoBackDone Let 0
EndIf
	
(10-1-3)[GoXPBeads]
AddToLogFile "Starting bead stir up"
AddToLogFile ">(10-1-3)"
RunMessage "Stirring up beads"
LiqIn $xpAirVol #P_SPEED_H
MoveTo XP_BEADS
Call LowerTips

(10-2-1)[StirUpBeads]
AddToLogFile ">(10-2-1)"
Gval xpTemp1 Let @WellVolume
Gval xpTemp1 * 0.6
If $xpTemp1 > $xpBeadMixVol
    Gval xpTemp1 Let $xpBeadMixVol
EndIf

; Since tips weren't lowered enough when the remaining vol
; of beads is low:
RScanZ $scanDist #Z_SPEED_L
RMove Z -300

Do $xpStirUps
    WaitMsec $mixDelay1
    LiqInWell $xpTemp1 #P_SPEED_M
    WaitMsec $mixDelay2
    LiqOutWell $xpTemp1 #P_SPEED_MH
Loop
Down% 20 #Z_SPEED_H
LiqOut $xpAirVol #P_SPEED_H
WaitSec $1sec
ActionZ #Z_SPEED_H W
StandbyZ #Z_SPEED_LL W
LiqIn $xpAirVol #P_SPEED_H
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume

(10-2-2)[StirUpBeads2]
AddToLogFile ">(10-2-2)"
Gval TimeCheck Let @SECONDCOUNT
Gval TimeCheck - $startTime
AddToLogFile "This is starttime " $startTime
AddToLogFile "This is timecheck " $TimeCheck
AddToLogFile "This is secondcount " @SECONDCOUNT
If $TimeCheck GT 21600
	AddToLogFile "Doing second stir up"
	Call LowerTips
	Gval xpTemp1 Let @WellVolume
	Gval xpTemp1 * 0.6
	If $xpTemp1 > $xpBeadMixVol
		Gval xpTemp1 Let $xpBeadMixVol
	EndIf
	RScanZ $scanDist #Z_SPEED_L
	RMove Z -300

	Do $xpStirUps
		WaitMsec $mixDelay1
		LiqInWell $xpTemp1 #P_SPEED_M
		WaitMsec $mixDelay2
		LiqOutWell $xpTemp1 #P_SPEED_MH
	Loop
	Down% 20 #Z_SPEED_H
	LiqOut $xpAirVol #P_SPEED_H
	WaitSec $1sec
	ActionZ #Z_SPEED_H W
	StandbyZ #Z_SPEED_LL W
	LiqIn $xpAirVol #P_SPEED_H
EndIf
	
(10-3-1)[SetInitTemp]
AddToLogFile ">(10-3-1)"
_SetTemp 2 $startTemp

(10-3-2)[GetAllBeads]
AddToLogFile ">(10-3-2)"
AddToLogFile "Collecting beads"
RunMessage "Collecting beads"
MoveTo XP_BEADS
Call LowerTipsMore
LiqInWell $cntXPBeadVol #P_SPEED_L
WaitSec $1sec
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume

(10-3-3)[GoFragSep]
AddToLogFile ">(10-3-3)"
If $DoSplit EQ 1
	MoveTo XTRAWORKPOS $DispIndex
Else
	MoveTo $cntXPStartingPos $DispIndex
EndIf

(10-3-4)[CheckXPTemp]
