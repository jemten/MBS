IfGoto <> 0 ;WAIT_LOOP
RunMessage "XP incubation temperature reached"

(10-3-5)[AddToSplit]
AddToLogFile ">(10-3-5)"
If $DoSplit EQ 1
	Down% 10 #Z_SPEED_H
	LiqOutWell $SplitBeadsVol #P_SPEED_M
	Down% 10 #Z_SPEED_H
	SideY% 90 #Y_SPEED_H
	SideY% -90 #Y_SPEED_H
	SideY% 0 #Y_SPEED_H
	MoveTo $cntXPStartingPos $DispIndex
EndIf

(10-3-6)[DispSample]
AddToLogFile ">(10-3-6)"
AddToLogFile "Dispensing beads to sample"
Call LowerTips
; #repl
LiqOutWell% 80 #P_SPEED_M
Down% 90 #Z_SPEED_H
LiqOutAll #P_SPEED_M
WaitMsec $500msec
LiqOut $xpAirVol #P_SPEED_H
Down% 10 #Z_SPEED_H
SideY% 90 #Y_SPEED_H
SideY% -90 #Y_SPEED_H
SideY% 0 #Y_SPEED_H
; Extra stuff to try and get out everything (101130):
StandbyZ #Z_SPEED_H
RMove Z 500 W
