(10-4-4)[StartingWatch]
AddToLogFile ">(10-4-4)"

; Starting watch
If $DispIndex EQ 1
	AddToLogFile "Starting watch: " $DispIndex
	StartWatch 1
EndIf
(10-5-1)[GoForSample2]
AddToLogFile ">(10-5-1)"
AddToLogFile "Going for Sample 2"
If $DispIndex EQ 1
	Gval DispIndex Inc
	Gval cntReserveID Let $cntTipID2
	Call ChangeReservedTip
	LiqIn $xpAirVol
	GoBack StirUpBeads2 
EndIf
Gval DispIndex Let 1
Gval cntReserveID Let $cntTipID1
Call ChangeReservedTip
LiqIn $xpAirVol

(10-5-2)[LoopWhileIncubating]
AddToLogFile ">(10-5-2)"
AddToLogFile "Incubating..."
RunMessage "Incubating..."

Gval xpTemp1 Let $nMixes
Gval xpTemp1 Inc
Gval mixInterval Let $xpBeadIncTime
Gval mixInterval \ $xpTemp1
Gval mixCount Let 0
Gval xpTemp1 Let $mixInterval
AddToLogFile "This is the mix interval: " $mixInterval

;INC_LOOP
PassTime $DispIndex
Gval Tempo1 Let @RESULT
Gval Tempo2 Let $xpBeadIncTime
Gval Tempo2 Sub $Tempo1
AddToLogFile $Tempo2 " seconds remaining of " $xpBeadIncTime
PassTime 1
If @Result < $xpBeadIncTime
    If $mixCount < $nMixes
        WaitTime 1 $xpTemp1
		;TAKE_TWO
        MoveTo $cntXPWorkingPos $DispIndex
        Call LowerTips
        Call GeneralMix
        Call ClearTipsXP
		If $DoSplit EQ 1
			MoveTo XTRAWORKPOS $DispIndex
			Call LowerTips
			Call GeneralMix
			Call ClearTipsXP
		EndIf
		If $DispIndex EQ 1
			MoveTo WATER $DispIndex
			Do 3 
				LiqIn 30 #P_SPEED_M
				LiqOut 30 #P_SPEED_M
			Loop
			WaitSec $1sec
			Down% 10 #Z_SPEED_M W
			LiqOut $xpAirVol #P_SPEED_H
			Gval DispIndex Inc
			Gval cntReserveID Let $cntTipID2
			Call ChangeReservedTip
			LiqIn $xpAirVol
			; Goto ;TAKE_TWO
			; Changing the goback loop since it might not be possible to search 
			; for TAKE_TWO when its inside an If statment
			MoveTo $cntXPWorkingPos $DispIndex
			Call LowerTips
			Call GeneralMix
			Call ClearTipsXP
			If $DoSplit EQ 1
				MoveTo XTRAWORKPOS $DispIndex
				Call LowerTips
				Call GeneralMix
				Call ClearTipsXP
			EndIf
			MoveTo WATER $DispIndex
			Do 3 
				LiqIn 30 #P_SPEED_M
				LiqOut 30 #P_SPEED_M
			Loop
			WaitSec $1sec
			Down% 10 #Z_SPEED_M W
			LiqOut $xpAirVol #P_SPEED_H
			Amove Z 0 W
			LiqIn $xpAirVol #P_SPEED_H
		EndIf
		Gval DispIndex Let 1
		Gval cntReserveID Let $cntTipID1
		Call ChangeReservedTip
		LiqIn $xpAirVol
        Gval mixCount Inc
        ; Next mix should be in <mixInterval> seconds from now
        PassTime 1
        Gval xpTemp1 Let @Result
        Gval xpTemp1 + $mixInterval
    Else
        WaitTime $DispIndex $xpBeadIncTime
    EndIf
    Goto ;INC_LOOP
EndIf

(10-6-1)[Capture_Beads]
AddToLogFile ">(10-6-1)"

(10-7-1)[Capture Part]
AddToLogFile ">(10-7-1)"
AddToLogFile "Staring bead capture"
RunMessage "Bead capture started"

(10-7-2)[GoFragSep]
AddToLogFile ">(10-7-2)"
If $GoBackDone EQ 0
	MoveTo XTRAWORKPOS $DispIndex
Else
	MoveTo $cntXPWorkingPos $DispIndex
EndIf

(10-7-3)[PreMixing]
AddToLogFile ">(10-7-3)"
Call LowerTips
Gval xpTemp1 Let @WellVolume
Gval xpTemp1 * 0.7
If $xpTemp1 > $xpBeadMixVol
    Gval xpTemp1 Let $xpBeadMixVol
EndIf
Do 10
  WaitMsec $mixDelay1
  LiqInWell $xpTemp1 #P_SPEED_MH
  WaitMsec $mixDelay2
  LiqOutWell $xpTemp1 #P_SPEED_MH
Loop

(10-7-4)[GetBeads1]
AddToLogFile ">(10-7-4)"
AddToLogFile "Get beads 1"
Gval xpTemp1 Let !@WellVolume(VolumeInWell)
If $xpTemp1 GT 200
	Gval xpTemp1 Div 2
	Gval TwoCaptures Let 1
EndIf
Call LowerTips
Gval xpTemp2 Let $xpTemp1
; xpTemp1 is 90% of the volume.
Gval xpTemp1 * 0.9
; xpTemp2 is 10% of the volume.
Gval xpTemp2 - $xpTemp1
Amove M $BF_MAG_HH
AddToLogFile "Aspirating 90% of well volume"
LiqInWell $xpTemp1 #P_SPEED_LL
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume
WaitMsec $500msec
ActionZ #Z_SPEED_H
RScanZ #DEL_SCAN_Z  #Z_SPEED_LL
DelMove Z #Z_DELUP_L W
AddToLogFile "Aspirating 10% of the volume"
LiqInWell $xpTemp2 #P_SPEED_LL
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume
Call LowerTips
AddToLogFile "Doing first mag_pass"

If $TwoCaptures EQ 1
	StandbyZ #Z_SPEED_M
	WaitMsec $500msec
EndIf

LiqIn #V_MAG_PASS #P_SPEED_LLL
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume
WaitMsec $500msec
LiqOut #V_MAG_PASS #P_SPEED_LLL

If $TwoCaptures EQ 1
	Call LowerTips
	WaitMsec $500msec
EndIf

LiqOutWell $xpTemp1 #P_SPEED_LLL
LiqOutWell $xpTemp2 #P_SPEED_LLL
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume

(10-7-5)[ReMix]
AddToLogFile ">(10-7-5)"
AddToLogFile "Mixing between captures"
Gval xpTemp1 Let @WellVolume
Gval xpTemp1 * 0.7
If $xpTemp1 > 30
    Gval xpTemp1 Let 30
EndIf
Do 5
    WaitMsec $mixDelay1
    LiqInWell $xpTemp1 #P_SPEED_M
    WaitMsec $mixDelay2
    LiqOutWell $xpTemp1 #P_SPEED_H
Loop
Amove M 0
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume

(10-7-6)[Action Z]
AddToLogFile ">(10-7-6)"
Call LowerTips

(10-7-7)[GetBeads1.2]
; Added a third bead capture to take all beads. 
; Remove 10-7-7 to 10-7-9 in order to restore
AddToLogFile ">(10-7-7)"
AddToLogFile "Get beads 1.2"
Gval xpTemp1 Let !@WellVolume(VolumeInWell)

If $xpTemp1 GT 200
	Gval xpTemp1 Div 2
	Gval TwoCaptures Let 1
EndIf

Call LowerTips
Gval xpTemp2 Let $xpTemp1
; xpTemp1 is 90% of the volume.
Gval xpTemp1 * 0.9
; xpTemp2 is 10% of the volume.
Gval xpTemp2 - $xpTemp1
Amove M $BF_MAG_HH
AddToLogFile "Aspirating 90% of well volume"
LiqInWell $xpTemp1 #P_SPEED_LL
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume
WaitMsec $500msec
ActionZ #Z_SPEED_H
RScanZ #DEL_SCAN_Z  #Z_SPEED_LL
DelMove Z #Z_DELUP_L W
AddToLogFile "Aspirating 10% of the volume"
LiqInWell $xpTemp2 #P_SPEED_LL
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume
Call LowerTips
AddToLogFile "Doing first mag_pass"

If $TwoCaptures EQ 1
	StandbyZ #Z_SPEED_M
	WaitMsec $500msec
EndIf

LiqIn #V_MAG_PASS #P_SPEED_LLL
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume
WaitMsec $500msec
LiqOut #V_MAG_PASS #P_SPEED_LLL

If $TwoCaptures EQ 1
	Call LowerTips
	WaitMsec $500msec
EndIf

LiqOutWell $xpTemp1 #P_SPEED_LLL
LiqOutWell $xpTemp2 #P_SPEED_LLL

AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume

(10-7-8)[ReMix]
AddToLogFile ">(10-7-8)"
AddToLogFile "Doing second remix"
Gval xpTemp1 Let @WellVolume
Gval xpTemp1 * 0.7
If $xpTemp1 > 30
    Gval xpTemp1 Let 30
EndIf
Do 5
    WaitMsec $mixDelay1
    LiqInWell $xpTemp1 #P_SPEED_M
    WaitMsec $mixDelay2
    LiqOutWell $xpTemp1 #P_SPEED_H
Loop
Amove M 0
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume

(10-7-9)[Action Z]
AddToLogFile ">(10-7-9)"
Call LowerTips

(10-7-10)[GetBeads2]
AddToLogFile ">(10-7-10)"
AddToLogFile "Get beads 2"
Gval xpTemp1 Let @WellVolume
If $xpTemp1 GT 200
	Gval xpTemp1 Div 2
EndIf
Gval xpTemp2 Let $xpTemp1
; Make xpTemp1 90% of the total volume.
Gval xpTemp1 * 0.9
; xpTemp2 is 10% of the volume.
Gval xpTemp2 - $xpTemp1
Amove M $BF_MAG_HH
AddToLogFile "Aspirating 90% of volume"
LiqInWell $xpTemp1 #P_SPEED_LL
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume
WaitSec $10sec
RScanZ #DEL_SCAN_Z  #Z_SPEED_LL
If @Result <> 0
	RMove Z -75 #Z_SPEED_L
; Retreat a shorter distance if bottom sensor isn't triggered:
Else
    RMove Z -50 #Z_SPEED_L
EndIf
AddToLogFile "Aspirating 10% of volume"
LiqInWell $xpTemp2 #P_SPEED_LL
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume
AddToLogFile "Aspirating xpExtraDisp, 10ul"
LiqIn $xpExtraDisp #P_SPEED_L
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume
Do 10
    DelMove Z -20 #Z_SPEED_H
    WaitMsec $mixDelay1
Loop

If $TwoCaptures EQ 1
	StandbyZ #Z_SPEED_M
Else 
	ActionZ #Z_SPEED_H
EndIf

AddToLogFile "Doing mag_pass"
LiqIn #V_MAG_PASS #P_SPEED_LLL
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume
WaitSec $10sec
If $DispIndex EQ 1
	MoveTo WASTE
Else 
	MoveTo WASTE2
EndIf
Down% 80 #Z_SPEED_H
LiqOut #V_MAG_PASS #P_SPEED_LLL
LiqOutWell $xpTemp1 #P_SPEED_LL
;To accommodate for MBS 8000 the Y position was changed to an absolute value in the other direction
AMove Y 17654 W
LiqOutWell $xpTemp2 #P_SPEED_LL
LiqOut $xpExtraDisp #P_SPEED_H
;SideY% -95 #Y_SPEED_M
LiqOut $xpAirVol #P_SPEED_H
Org P
SideY% 0 #Y_SPEED_H
AMove M 0 W
AMove Z 0 W
LiqIn $xpAirVol #P_SPEED_H
Org Z
Org M
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume
If $TwoCaptures EQ 1
	AddToLogFile "Going back for the rest of the beads"
	GVal TwoCaptures Let 0
	MoveTo $cntXPWorkingPos $DispIndex
	GoBack GetBeads1.2
EndIf
If $GoBackDone EQ 0
	MoveTo $cntXPWorkingPos $DispIndex
	Gval GoBackDone Let 1
	If @WellVolume GT 10
		GoBack PreMixing
	EndIf
EndIf

